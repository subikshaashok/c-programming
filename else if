//EX
#include <stdio.h>
int main(){
int time = 20;
if (time >20){
printf("subiksha");
}else if (time =20){
printf("SUBIKSHA ASHOK");
}else{
printf("ASHOK");
}
}

//EX-2
#include <stdio.h>
int main(){
int time = 22;
if (time < 10) {
  printf("Good morning.");
} else if (time < 20) {
  printf("Good day.");
} else {
  printf("Good evening.");
}
}

 //OUTPUT good evening
