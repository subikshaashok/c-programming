#include <stdio.h>
int main(){
// can overwrite this like this 
int mynum=10; 
mynum=12;
printf("%d",mynum);
return 0;
}
//You can also assign the value of one variable to another
#include <stdio.h>
int main(){
int mynum=10; 
int othermynum=12;
//You can also assign the value of one variable to another:
mynum=othermynum;
printf("%d",mynum);
return 0;
}
//both output are 12
